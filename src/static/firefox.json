{
  "manifest_version": 3,
  "name": "Extension - Starter",
  "description": "Extension - Starter",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extension - Starter",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
